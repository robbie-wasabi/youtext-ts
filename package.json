{
  "name": "youtext",
  "version": "1.0.0",
  "description": "A Streamlined YouTube Transcript Extractor",
  "main": "dist/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "tsc && node dist/index.js",
    "nodemon": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node --loader ts-node/esm' src/index.ts"
  },
  "type": "commonjs",
  "jest": {
    "testPathDirs": [
      "__tests__"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "es6"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quokkaine/youtext.git"
  },
  "keywords": [
    "youtube",
    "text",
    "youtext",
    "transcript"
  ],
  "author": "robbie_wasabi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/quokkaine/youtext/issues"
  },
  "homepage": "https://github.com/quokkaine/youtext#readme",
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "babel-jest": "^29.5.0",
    "compromise": "^14.9.0",
    "dotenv": "^16.0.3",
    "es6-promise": "^4.2.8",
    "express": "^4.18.2",
    "firebase": "^9.23.0",
    "fs": "^0.0.1-security",
    "googleapis": "^118.0.0",
    "ipfs-http-client": "^60.0.0",
    "openai": "3.3",
    "promise": "^8.3.0",
    "youtube-transcript": "^1.0.5"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
    "@types/express": "^4.17.21",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "typescript": "^4.5.4",
    "ts-node": "^10.4.0",
    "@types/node": "^17.0.8"
  }
}